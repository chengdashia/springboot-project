<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cloudDisk.mapper.FileDelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.cloudDisk.pojo.FileDel">
        <id column="file_id" property="fileId" />
        <result column="file_del_id" property="fileDelId" />
        <result column="user_id" property="userId" />
        <result column="file_del_time" property="fileDelTime" />
        <result column="file_del_status" property="fileDelStatus" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        file_del_id, file_id, user_id, file_del_time, file_del_status
    </sql>

</mapper>
